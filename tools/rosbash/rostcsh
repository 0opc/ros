alias roscd 'cd `rospack find \!*; if ($?) echo $ROS_ROOT`' 
alias rcd roscd
alias rosls 'ls `rospack find \!*`'
alias rosd 'echo "unimplemented"'
alias rospd 'echo "unimplemented"'
alias rosed 'echo "unimplemented"'
alias roscmd 'echo "unimplemented"'
alias roscp 'echo "unimplemented"'

complete roscd 'p/1/`rospack list-names`/' 
complete rosmake 'p/1/`rospack list-names`/' 
complete rosupdate 'p/1/`rospack list-names`/' 
complete rosapt-get 'p/*/`rospack list-names`/' 
complete rosdeb 'p/*/`rospack list-names`/' 

complete rospack \
   'p/1/(help find list list-names langs deps deps-manifests deps1 deps-indent sysdeps sysdeps0 vcs vcs0 depends-on)/' \
   'n/(find deps deps-manifests deps1 deps-indent sysdeps sysdeps0 vcs vcs0 depends-on depends-on1 export cflags-only-I cflags-only-other libs-only-L libs-only-l libs-only-other)/`rospack list-names`/' 




